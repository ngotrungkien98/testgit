@model IEnumerable<WADLab9.Models.Customer>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Customer ID
            </th>
            <th>
                Customer Name
            </th>
            <th>
                Address
            </th>
            <th>
                Phone
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in ViewBag.cusList) {
        <tr>

            <td>
                @item.customerid
            </td>
            <td>
                @item.customername
            </td>
            <td>
                @item.customeraddress
            </td>
            <td>
                @item.customerphone
            </td>
            <td>

                <a asp-action="Edit" asp-route-id="@item.customerid">Edit or Delete</a> |

            </td>
        </tr>
}
    </tbody>
</table>
